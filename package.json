{
  "name": "js_version",
  "version": "2.6.0",
  "changelogs": [
    {
      "version": "2.6.0",
      "changelog": "Temporarily removed subscriptions due to frequent bugs"
    },
    {
      "version": "2.5.0",
      "changelog": "Added logging for ModBot actions! You can set it up with `m: logging enable`"
    },
    {
      "version": "2.4.0",
      "changelog": "Fixed confusing and super long ModBot syntax errors! When you make an error, ModBot will now show you exactly how to correct it"
    },
    {
      "version": "2.3.0",
      "changelog": "Added ability to cancel reminders"
    },
    {
      "version": "2.2.0",
      "changelog": "Added custom command prefixes! You can set them up with `m: prefix add`"
    },
    {
      "version": "2.1.0",
      "changelog": "Enabled more compiler checks, continuing to substantially improve reliability"
    },
    {
      "version": "2.0.1",
      "changelog": "Rewrote moderation submodule into typescript"
    },
    {
      "version": "2.0.0",
      "changelog": "Rewrote 2.5 thousand lines of code into typescript. Vastly reduced the number of unexplained errors that will be displayed"
    }
  ],
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "npm run clear && npm run eslint && npm run nearley && npm run tsc && npm run clear",
    "tsc": "copyfiles package.json build && copyfiles credentials.json build && tsc",
    "nearley": "nearleyc ./commands.ne -o ./commands.js",
    "eslint": "npx eslint . --ext .js,.ts",
    "build": "npm run clear && npm run nearley && npm run tsc",
    "dev": "npm run nearley && ts-node-dev --respawn ./main.ts",
    "clear": "npx del-cli build/ commands.js"
  },
